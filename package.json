{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "deno": "deno",
    "dev": "remix build && run-p dev:*",
    "dev:deno": "cross-env NODE_ENV=development deno run --unstable --watch --allow-net --allow-read --allow-env ./build/index.js",
    "dev:css": "npm run css -- --watch",
    "dev:remix": "remix watch",
    "build": "run-s build:*",
    "build:css": "npm run css -- --minify",
    "build:remix": "remix build",
    "start": "cross-env NODE_ENV=production deno run --unstable --allow-net --allow-read --allow-env ./build/index.js",
    "format": "deno fmt",
    "lint": "deno lint",
    "css": "tailwindcss -i ./app/styles.css -o ./app/styles.output.css",
    "postinstall": "touch ./app/styles.output.css"
  },
  "dependencies": {
    "@remix-run/deno": "1.7.5",
    "@remix-run/react": "1.7.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-utils": "4.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.7.5",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4"
  },
  "engines": {
    "node": ">=14"
  }
}
