{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "deno": "deno",
    "dev": "remix build && run-p dev:*",
    "dev:deno": "cross-env NODE_ENV=development deno run --unstable --watch --allow-net --allow-read --allow-env ./build/index.js",
    "dev:windi": "npm run css -- --dev",
    "dev:remix": "remix watch",
    "build": "run-s build:*",
    "build:windi": "npm run css -- --minify",
    "build:remix": "remix build",
    "start": "cross-env NODE_ENV=production deno run --unstable --allow-net --allow-read --allow-env ./build/index.js",
    "format": "deno fmt",
    "lint": "deno lint",
    "css": "windicss './app/**/*.tsx' -to ./app/styles.css -f windi.config.js",
    "postinstall": "touch ./app/styles.css"
  },
  "dependencies": {
    "@remix-run/deno": "1.7.2",
    "@remix-run/react": "1.7.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.7.2",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "windicss": "^3.5.6"
  },
  "engines": {
    "node": ">=14"
  }
}
